@{
}
@model IEnumerable<string>
<div class="d-grid gap-2 ">
<a asp-controller="Home" asp-action="Index" asp-route-projectHi="" asp-route-pageNum ="" class="btn btn-outline-secondary"> Home</a>
@foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-projectHi="@t" asp-route-pageNum="" class="btn @( ViewBag.SelectedType == t ? "btn-primary":"btn-outline-secondary")">@t</a>
    }
</div>